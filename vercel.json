{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
      {
        "src": "/(.*)",
        "dest": "server/index.js"

      },
      { "src": "/(.*)", "dest": "/index.html" } 
    ]
  }
 