{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/favicon.ico",
        "dest": "/client/dist/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "/client/dist/$1"
      }
    ]
  }
  