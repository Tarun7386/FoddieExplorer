{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/dist",
          "buildCommand": "npm install vite && npm run build"  
        }
      },
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      
      {
        "src": "/(.*)",
        "dest": "server/index.js"
      }
    ]
  }
  